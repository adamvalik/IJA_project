<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="menuPane" prefHeight="522.0" prefWidth="772.0" style="-fx-background-color: #d6d6d6;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ija.controller.MenuController">
   <Button fx:id="newGame" alignment="CENTER" layoutX="284.0" layoutY="208.0" mnemonicParsing="false" prefHeight="59.0"
           prefWidth="205.0"
           style="-fx-background-color: #FFD100; -fx-border-color: #202020; -fx-border-width: 3px; -fx-background-radius: 20px; -fx-border-radius: 17px;"
           text="NEW GAME">
      <font>
         <Font name="Arial Black" size="22.0"/>
      </font>
   </Button>
   <Button fx:id="loadGame" layoutX="284.0" layoutY="298.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="205.0"
           style="-fx-background-color: #FFD100; -fx-border-color: #202020; -fx-border-width: 3px; -fx-background-radius: 20px; -fx-border-radius: 17px;"
           text="LOAD GAME">
      <font>
         <Font name="Arial Black" size="22.0"/>
      </font>
   </Button>
   <Button fx:id="endGame" layoutX="284.0" layoutY="388.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="205.0"
           style="-fx-background-color: #FFD100; -fx-border-color: #202020; -fx-border-width: 3px; -fx-background-radius: 20px; -fx-border-radius: 17px;"
           text="QUIT GAME">
      <font>
         <Font name="Arial Black" size="22.0"/>
      </font>
   </Button>
   <Text fx:id="text" layoutX="47.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0"
         text="ULTIMATE ROBOT/RACING GAME" textAlignment="CENTER">
      <font>
         <Font name="Arial Black" size="37.0"/>
      </font>
   </Text>
</AnchorPane>
